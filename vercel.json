{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/index.ts"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/api",
      "permanent": false
    }
  ],
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}